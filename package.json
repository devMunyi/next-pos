{
  "name": "@zap-ts/core",
  "version": "1.0.0",
  "private": false,
  "description": "Core package for Zap.ts, a TypeScript framework for building web applications.",
  "author": "Alexandre Trotel",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "postbuild": "next-sitemap",
    "clean": "rimraf .next dist .vitepress",
    "db:check": "drizzle-kit check",
    "db:export": "drizzle-kit export",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:up": "drizzle-kit up",
    "dev": "next dev --turbopack",
    "format": "prettier --write .",
    "lint": "next lint",
    "mail:dev": "email dev",
    "scan": "next dev --turbopack & npx react-scan@latest localhost:3000",
    "start": "next start",
    "zap": "npx create-zap-app@latest",
    "db:seed": "bun run src/zap/db/seed/index.ts",
    "db:resetx": "drizzle-kit push && bun run src/zap/db/seed/index.ts"
  },
  "lint-staged": {
    "*": [
      "next lint --fix .",
      "prettier --write ."
    ]
  },
